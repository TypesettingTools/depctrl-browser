---
pagination:
    data: feeds
    size: 1
    alias: feed
permalink: "feed/{{ feed.id | slug }}/"
---

<h1>{{ feed.name }}</h1>
By {{ feed.maintainer }}
<p>{{ feed.description }}</p>

<h3>Macros:</h3>
{% for macroId, macro in feed.macros %}
  <li><a href="/feed/{{ feed.id | slug }}/macro/{{ macroId | slug }}/">{{ macro.name }}</a> by {{ macro.author }}</li>
{% else %}
  <li>No Items</li>
{% endfor %}

<h3>Modules:</h3>
{% for moduleId, module in feed.modules %}
  <li><a href="/feed/{{ module.feedId | slug }}/module/{{ moduleId | slug }}/">{{ module.name }}</a> by {{ module.author }}</li>
{% else %}
  <li>No Items</li>
{% endfor %}
